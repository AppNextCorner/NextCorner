{
  "expo": {
    "name": "NextCornerApp",
    "slug": "nextcornerapp",
    "owner": "nextcornerapplication",
    "version": "1.0.0",
    "production": {
      "channel": "Developers/MVP-Iteration"
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#78DBFF"
    },

    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.nextcorner.NextCornerApp",
      "config": {
        "googleMapsApiKey": ""
      }
    }
  },
  "android": {
    "package": "com.nextcorner.NextCornerAPP",
    "adaptiveIcon": {
      "foregroundImage": "./assets/adaptive-icon.png",
      "backgroundColor": "#FFFFFF"
    }
  },
  "web": {
    "favicon": "./assets/favicon.png"
  }
}
